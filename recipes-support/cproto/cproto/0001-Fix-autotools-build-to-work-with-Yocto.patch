+srcdir = @srcdir@
+
+SUFFIXES = .c .h .y .l
+ACLOCAL_AMFLAGS = -I m4
+#BUILT_SOURCES = grammar.h
+#AM_YFLAGS = -d
+bin_PROGRAMS = cproto
+cproto_SOURCES = \
+	cproto.c \
+	lintlibs.c \
+	semantic.c \
+	strkey.c \
+	symbol.c \
+	$(srcdir)/grammar.c
